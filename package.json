{
  "name": "@mj-studio/js-util",
  "version": "1.0.13",
  "description": "Custom JavaScript Utilities for MJ Studio",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/mym0404/mj-studio-js-util",
  "scripts": {
    "prepare": "yarn c",
    "c": "yarn lint && yarn tsc && yarn jest --coverage -u",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "test": "jest",
    "p": "yarn publish --access=public"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mym0404/mj-studio-js-util"
  },
  "author": "MJ Studio <mym0404@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@react-native-community/eslint-config": "3.2.0",
    "@types/jest": "^26.0.9",
    "eslint": "8.34.0",
    "import-sort-style-eslint": "6.0.0",
    "jest": "^26.3.0",
    "prettier": "2.8.4",
    "prettier-plugin-import-sort": "0.0.7",
    "ts-jest": "26.4.4",
    "typescript": "4.9.5"
  },
  "importSort": {
    ".js, .jsx": {
      "style": "eslint",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  },
  "files": [
    "dist",
    "package.json"
  ]
}
