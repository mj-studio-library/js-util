{
  "name": "@mj-studio/js-util",
  "version": "0.0.7",
  "description": "Custom JavaScript Utilities for MJ Studio",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/mym0404/mj-studio-js-util",
  "scripts": {
    "prepare": "yarn c",
    "c": "yarn lint && yarn tsc && yarn jest --coverage -u",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "test": "jest",
    "p": "yarn publish --access=public"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mym0404/mj-studio-js-util"
  },
  "author": "MJ Studio <mym0404@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@dooboo/eslint-config": "^0.4.3",
    "@types/eslint": "^7.2.0",
    "@types/jest": "^26.0.9",
    "eslint": "^7.6.0",
    "import-sort-style-eslint": "^6.0.0",
    "jest": "^26.3.0",
    "prettier": "^2.0.5",
    "prettier-plugin-import-sort": "^0.0.4",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  },
  "importSort": {
    ".js, .jsx": {
      "style": "eslint",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  },
  "files": [
    "dist",
    "package.json"
  ]
}
