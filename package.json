{
  "name": "@mj-studio/js-util",
  "version": "1.1.22",
  "description": "Custom JavaScript Utilities for MJ Studio",
  "homepage": "https://github.com/mj-studio-library/js-util",
  "scripts": {
    "t": "pnpm lint && pnpm tsc && pnpm test -- --coverage",
    "tsc": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "test": "vitest --run",
    "release": "zx tool/publish.mjs",
    "build": "tsdown"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mym0404/mj-studio-js-util"
  },
  "author": "MJ Studio <mym0404@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@mj-studio/eslint-config-node": "^2.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zx": "^8.8.1"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
  "main": "dist/index.js",
  "module": "dist/js-util.esm.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "typings": "dist/index.d.ts"
}