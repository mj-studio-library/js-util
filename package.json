{
  "name": "@mj-studio/js-util",
  "version": "1.1.20",
  "description": "Custom JavaScript Utilities for MJ Studio",
  "homepage": "https://github.com/mj-studio-library/js-util",
  "scripts": {
    "t": "pnpm lint && pnpm tsc && pnpm test -- --coverage",
    "tsc": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "test": "vitest --run",
    "release": "zx tool/publish.mjs",
    "build": "tsdx build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mym0404/mj-studio-js-util"
  },
  "author": "MJ Studio <mym0404@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@mj-studio/eslint-config-node": "^2.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "tsdx": "^0.14.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "packageManager": "pnpm@9.10.0",
  "main": "dist/index.js",
  "module": "dist/js-util.esm.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/js-util.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typings": "dist/index.d.ts"
}
